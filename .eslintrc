{
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": 1,
    "no-debugger": 1,
    "no-alert": 1,
    "react/prop-types": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-deprecated": 2,
    "react/prefer-es6-class": 2
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "expect": true
  }
}
